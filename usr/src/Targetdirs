sparc_64ONLY= $(POUND_SIGN)
64ONLY=  $($(MACH)_64ONLY)

aarch64_DIRS=				\
	/usr/lib/inet/$(MACH)		\
	/usr/bin/$(MACH)		\
	/usr/lib/rcap/$(MACH)		\
	/usr/sbin/$(MACH)

alpha_DIRS=				\
	/usr/lib/inet/$(MACH)		\
	/usr/bin/$(MACH)		\
	/usr/lib/rcap/$(MACH)		\
	/usr/sbin/$(MACH)

ifeq ($(MACH),$(MACH64))
$(MACH)_DIRS += /usr/ucb/$(MACH)
else
$(64ONLY)MACH32_DIRS=/usr/ucb/$(MACH32)
endif

